<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Login</title>
    <%- include('importcss.ejs') %>


</head>
<body style="background-color:whitesmoke"     >

    <div class="container" style="margin-top: 80px" >
        <div class="row "   >
            <div class="col-sm-1 col-md-2 col-lg-3 col-0"></div>
            <div class="col-sm-10 col-md-8 col-lg-6 col-12" >
                <div class="card">
                    <div class="card-header"  style="text-align: center">
                        <h4>Login</h4>
                    </div>
            <form   method="POST">

                    <div class="card-body">
                        <div class="form-group">
                            <label class="col-form-label pt-0" for="exampleInputEmail1">Email address</label>
                            <input class="form-control" name="email" id="exampleInputEmail1" type="email" placeholder="Enter email" data-original-title="" title="">
                        </div>
                        <div class="form-group">
                            <label class="col-form-label pt-0" for="PasswordInput">Password</label>
                            <input class="form-control" id="PasswordInput" type="password" placeholder="Enter Password" name="password" data-original-title="" title="">
                        </div>
                    </div>
                    <div class="card-footer">
                        <button type="submit" class="btn btn-primary " style="width: 100%" >Login</button>
                    </div>
        </form>

                </div>

            </div>
            <div class="col-sm-1 col-md-2 col-lg-3 col-0"></div>
        </div>

    </div>


    <%- include('importjs.ejs')%>

</body>
</html>
